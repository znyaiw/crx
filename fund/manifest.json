{
    "manifest_version": 2,
    "name": "Fund-基金助手",
    "version": "0.1.0",
    "description": "Fund-基金助手",
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "icons/icon_16.png",
            "32": "icons/icon_32.png",
            "48": "icons/icon_48.png",
            "128": "icons/icon_128.png"
        },
        "default_title": "Fund-基金助手",
        "default_popup": "index.html"
    },
    "permissions": [
        "declarativeContent",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://fundgz.1234567.com.cn/; object-src 'self'",
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "commands": {
        "_execute_page_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+A",
                "mac": "Command+Shift+A"
            }
        }
    }
}
